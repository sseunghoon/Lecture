package first_project;

import java.util.Scanner;

class Circle {
	int x;
	int y;
	int r;
}

public class Hw1_1 {
	public static void main(String args[]) {
	
	Scanner scanner = new Scanner(System.in);
	int arr1[] = new int[3];
	int arr2[] = new int[3];
	
	for (int i = 0; i < arr1.length(); i++) {
		arr1[i] = scanner.nextInt();
	}
	for (i = 0; i < arr2.length(); i++) {
		arr1[i] = scanner.nextInt();
	}
	
	scanner.close();
	
	}
}
