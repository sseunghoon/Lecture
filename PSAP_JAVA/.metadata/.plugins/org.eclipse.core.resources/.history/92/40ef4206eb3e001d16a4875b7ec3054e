package first_project;

import java.util.Scanner;

class Circle {
	int x;
	int y;
	int r;
}

public class Hw1_1 {
	public static void main(String args[]) {
	
	Scanner scanner = new Scanner(System.in);
	int arr1[] = new int[3];
	int arr2[] = new int[3];
	
	System.out.println(arr1.length + "개의 정수를 입력하세요.");
	for (int i = 0; i < 3; i++) {
		arr1[i] = scanner.nextInt();
	}
	System.out.println(arr2.length + "개의 정수를 입력하세요.");
	for (int i = 0; i < 3; i++) {
		arr2[i] = scanner.nextInt();
	}
	
	scanner.close();
	
	}
}
